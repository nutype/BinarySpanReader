<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <PackageId>BinarySpanReader</PackageId>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>nutype</Authors>
    <Company />
    <Product>BinarySpanReader</Product>
    <Description>Binary Reader for Span&lt;T&gt;.  As of right now, this consists of various
extension methods to ReadOnlySpan&lt;byte&gt;.</Description>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Copyright>2021</Copyright>
    <RepositoryUrl>https://github.com/nutype/BinarySpanReader</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>span; memory; binaryreader; readonlyspan</PackageTags>
    <NeutralLanguage />
    <Nullable>enable</Nullable>
    <Version>1.0.2</Version>
    <AssemblyVersion>1.0.2.0</AssemblyVersion>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
    <DocumentationFile></DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
